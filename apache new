<VirtualHost *:80>
     ServerAdmin admin@example.com
     DocumentRoot /var/www/wordpress1/
     ServerName zuoweiyi.me
     ServerAlias www.zuoweiyi.me

     ErrorLog ${APACHE_LOG_DIR}/error1.log
     CustomLog ${APACHE_LOG_DIR}/access1.log combined

</VirtualHost>

